{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@latest"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html"
    },
    {
      "src": "/demo",
      "dest": "/public/demo.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
