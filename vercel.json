{
  "rewrites": [
    {
      "source": "/login",
      "destination": "/public/login.html"
    },
    {
      "source": "/admin",
      "destination": "/public/admin.html"
    }
  ],
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
